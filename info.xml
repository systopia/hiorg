<?xml version="1.0"?>
<extension key="hiorg" type="module">
  <file>hiorg</file>
  <name>HiOrg-Server API</name>
  <description>An API client for connecting to a HiOrg-Server instance.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/hiorg</url>
    <url desc="Documentation">https://github.com/systopia/hiorg</url>
    <url desc="Support">https://github.com/systopia/hiorg</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-07-11</releaseDate>
  <version>1.0-beta2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.71</ver>
  </compatibility>
  <comments>See https://info.hiorg-server.de for information on HiOrg-Server.</comments>
  <requires>
    <ext>config-profiles</ext>
    <ext>oauth-client</ext>
    <ext>de.systopia.xcm</ext>
    <ext>de.systopia.identitytracker</ext>
  </requires>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Hiorg</namespace>
    <format>23.02.1</format>
    <angularModule>crmHiorg</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
